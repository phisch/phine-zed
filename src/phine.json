        "toolbar.background": "$surface.primary",
        "tab_bar.background": "$surface.secondary",
        "tab.inactive_background": "$surface.primary",
        "tab.active_background": "$surface.primary",
        "search.match_background": "#74ade866",
        "panel.background": "$surface.secondary",
        "panel.focused_border": null,
        "pane.focused_border": null,
        "scrollbar.thumb.background": "$state.active",
        "scrollbar.thumb.hover_background": "$debug",
        "scrollbar.thumb.border": "$surface.transparent",
        "editor.indent_guide": "$guide.default",
        "editor.indent_guide_active": "$guide.active",
        "panel.indent_guide": "$guide.default",
