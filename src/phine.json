      "name": "Phine",
      "style": {
        "border": "$surface.tertiary",
        "border.variant": "$surface.tertiary",
        "border.focused": "#293b5bff",
        "border.selected": "#293b5bff",
        "border.transparent": "#00000000",
        "border.disabled": "#414754ff",
        "elevated_surface.background": "$surface.modal",
        "surface.background": "$surface.secondary",
        "background": "$surface.primary",
        "element.background": "$state.hover",
        "element.hover": "$surface.modal",
        "element.active": "#454a56ff",
        "element.selected": "#454a56ff",
        "element.disabled": "#2e343eff",
        "drop_target.background": "$surface.overlay",
        "ghost_element.background": "$surface.transparent",
        "ghost_element.hover": "$state.hover",
        "ghost_element.active": "$state.active",
        "ghost_element.selected": "$state.selected",
        "ghost_element.disabled": "$state.disabled",
        "text": "$text.default",
        "text.muted": "$text.muted",
        "text.placeholder": "$text.placeholder",
        "text.disabled": "$text.disabled",
        "text.accent": "$text.syntax.yellow",
        "icon": "$text.default",
        "icon.muted": "#838994ff",
        "icon.disabled": "#696B77ff",
        "icon.placeholder": "#838994ff",
        "icon.accent": "#74ade8ff",
